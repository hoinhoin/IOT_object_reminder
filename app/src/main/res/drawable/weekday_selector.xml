<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">

    <!-- 하늘색 배경 설정 -->
    <solid android:color="#87CEEB" />

    <!-- 테두리 설정 (선택적으로 추가 가능) -->
